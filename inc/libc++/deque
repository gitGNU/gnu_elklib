#ifndef ELKLIB_LIBCXX_DEQUE
#define ELKLIB_LIBCXX_DEQUE

#include "elklib.h"
#include "libc++/iterator"

namespace ktl
{
	template<typename T> class deque {
	public:
		container();
		container(const container & c );
		container(size_type num, const T & val = T());
		container(input_iterator start, input_iterator end);
		~container();

	protected:

	private:
	};
}

#endif // ELKLIB_LIBCXX_DEQUE
